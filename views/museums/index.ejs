<h2 class="text-center">Museums List</h2>




<% museums.forEach(function(m) { %>
<div>
    <h1><a href="/museums/<%=m._id%>"> <%=m.name  %></a></h1>
    <p><%=m.city  %> </p>
    <p><%=m.country  %> </p>
    <img src=<%=m.image%> alt="museum">

    <form method="POST" action="/museums/<%=m.id%>?_method=DELETE">
        <input type="submit"  value="Delete Museum">
    </form>
    <hr/>

</div>
 <% }); %>
