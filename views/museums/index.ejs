<h2 class="text-center">Museums List</h2>

<!-- DELETE CODE BELOW THIS LINE -->

<% museum.forEach(function(m) { %>
<div class="card mb-3">
  <img class="card-img-top" src="<%= m.image %>" alt="Card image cap">
  <div class="card-body">
    <h5 class="card-title"><a href="museums/<%=m.id%>"><%= m.name %></a></h5>
    <p class="card-text">
    	<%= m.city %>, <%= m.country %>
    	<ul>
    		<li></li>
    	</ul>
    </p>
    <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
  </div>
</div>

	
<% }) %>
