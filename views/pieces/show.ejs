<h2>TODO: Show a specific Piece</h2>

<% pieces.forEach(p => { %>
	<div>
		<h2><%= p.name %></h2>
		<img src="<% p.image %>">
		<h3>Creator</h3>
		<p><%= p.creator.firstname %><% p.creator.lastname %></p>
		<img src="<%= p.creator.image %>">
		<p>Birth: <%= p.creator.birthyear %></p>
		<p>Birth: <%= p.creator.deathyear %></p>
		<p>At the
			<a href="/museums/<%= p.museum.id %>">
				<%= p.museum.name %>
			</a>
		</p>
	</div>
<%})%>
